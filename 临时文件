 select 
  evaluationrecord.createtime, regression.version, caseid, testcase.casename, 
  indication.name, indication.comment, cast(evaluationrecord.indicationvalue as decimal(18,2)) as value
from evaluationrecord 
  left join indication on evaluationrecord.indicationid= indication.id 
  left join testcase on evaluationrecord.caseid=testcase.id 
  left join regression on evaluationrecord.regressionid = regression.id 
where 
  indication.id != 4
  and indication.id != 6
  and indication.id != 8
  and evaluationrecord.indicationvalue between -10000.0 and 10000.0



 select 
  evaluationrecord.createtime, caseid, testcase.casename, 
  indication.name, indication.comment, evaluationrecord.indicationvalue
from evaluationrecord 
  left join indication on evaluationrecord.indicationid= indication.id 
  left join testcase on evaluationrecord.caseid=testcase.id 
  left join regression on evaluationrecord.regressionid = regression.id 
where 
  indication.id != 4
  and indication.id != 6
  and indication.id != 8;


  indication.name in ($indication) 
  and testcase.casename in ($testcase)
  and regression.version in ($regressionid);



SELECT
  createtime AS "time",
  value
FROM view_for_visualization
WHERE
  $__timeFilter(createtime)
  and name in ($indication) 
  and casename in ($testcase)
ORDER BY createtime


SELECT
  createtime AS "time",
  value
FROM view_for_visualization
WHERE
  $__timeFilter(createtime)
  and name in ($indication)
  and casename ="cases/test_DEWHILEMS-E7"
ORDER BY createtime
